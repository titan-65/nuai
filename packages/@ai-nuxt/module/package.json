{
  "name": "@ai-nuxt/module",
  "version": "0.1.0",
  "description": "Main Nuxt module for AI Nuxt framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist",
    "runtime"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-nuxt/core": "workspace:*",
    "@nuxt/kit": "^3.9.0",
    "defu": "^6.1.4",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/schema": "^3.9.0",
    "nuxt": "^3.9.0",
    "typescript": "^5.3.0",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "nuxt": "^3.9.0"
  }
}